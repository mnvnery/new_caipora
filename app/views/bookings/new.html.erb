<main>
<%= simple_form_for [@trip, @booking] do |f| %>
<%= f.simple_fields_for :client do |r| %>
    <%= r.input :name %>
    <%= r.input :email %>
    <%= r.input :phone %>
    <%= r.input :NIF %>
    <%= r.input :address %>
    <%= r.input :post_code %>
    <%= r.input :city %>
<% end %>

    <%= f.submit "Validate", class: "btn btn-primary"%>

<% end %>

</main>